{% for dim in dim_contexts %}
    <CalculatedMember dimension="Measures" name="{{ value_label_title }}, Percent by {{ dim.title }}">
        <Annotations>
            <Annotation name="percent_for">{{ dim.title }}</Annotation>
        </Annotations>
        <Formula>
          ([Measures].[{{ value_label_title }}] / ([{{ dim.title }}].[{{ dim.title }}].[All {{ dim.title }}s], [Measures].[{{ value_label_title }}]))
        </Formula>
    </CalculatedMember>
{% endfor %}
